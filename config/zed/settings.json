{
  "show_edit_predictions": true,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "agent": {
    "always_allow_tool_actions": true,
    "single_file_review": true
  },
  "icon_theme": "Bearded Icon Theme",
  "base_keymap": "Cursor",
  "ui_font_size": 18.0,
  "buffer_font_size": 16.0,
  "buffer_font_family": "Monaspace Argon",
  "terminal": {
    "font_family": "Maple Mono NF"
  },
  "autosave": "on_focus_change",
  "theme": {
    "mode": "system",
    "light": "Zedokai Light (Filter Sun)",
    "dark": "Zedokai Darker (Filter Ristretto)"
  },
  "auto_install_extensions": {
    "bearded": true,
    "bearded-icon-theme": true,
    "dockerfile": true,
    "git-firefly": true,
    "html": true,
    "latex": true,
    "make": true,
    "mcp-server-context7": true,
    "toml": true,
    "zedokai": true
  }
}
